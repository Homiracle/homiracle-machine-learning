{
    "builds": [{
        "src": "server/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python 3.11.2"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/wsgi.py"
        }
    ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
    ]
}

